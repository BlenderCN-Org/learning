.amp-viewer-header { transform: translate3d(0px, 0px, 0px); display: flex; align-items: center; height: 50px; background: linear-gradient(rgb(243, 243, 243) 0%, rgb(239, 239, 239) 100%); border-bottom: 1px solid rgb(223, 223, 223); z-index: 1; box-shadow: rgba(0, 0, 0, 0.2) 0px 1px 1px; position: fixed; width: 100%; }

