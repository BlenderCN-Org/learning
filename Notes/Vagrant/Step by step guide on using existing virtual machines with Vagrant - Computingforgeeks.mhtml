.amp-viewer-header .amp-viewer-icon-arrow-left { width: 15px; height: 15px; padding: 5px; }

.amp-viewer-header .amp-viewer-icon-close { width: 15px; height: 15px; padding: 15px; }

.amp-viewer-header .amp-viewer-header-main { display: flex; flex: 1 1 0%; align-items: center; flex-direction: column; font-size: 13px; line-height: 12px; }

.amp-viewer-header .amp-viewer-icon + .amp-viewer-header-main { margin-right: 45px; }

.amp-viewer-body, .amp-viewer-frame, .amp-viewer-frame-custom-markup { padding-top: 50px; }

.amp-viewer-body { position: relative; overflow: hidden; background: rgb(51, 51, 51); box-sizing: content-box; height: 100%; }

.amp-viewer-frame { transform: translate3d(0px, 0px, 0px); position: fixed; background: rgb(51, 51, 51); top: 0px; left: 0px; width: 100%; height: 100%; overflow: auto; }

