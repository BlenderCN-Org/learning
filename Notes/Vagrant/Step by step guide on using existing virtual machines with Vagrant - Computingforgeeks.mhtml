.amp-viewer-url-indicator > li.amp-viewer-url-indicator-active { background: rgb(76, 76, 76); }

.amp-viewer-loading-indicator { display: inline-block; width: 40px; height: 40px; transform: translate3d(0px, 0px, 0px); }

.amp-viewer-loading-indicator .amp-viewer-loading-indicator-circle { fill: transparent; animation: 1.5s linear 0s infinite normal none running amp-viewer-trace; transform-origin: center center; stroke: rgb(153, 153, 153); stroke-width: 3px; stroke-linecap: round; stroke-dasharray: 113px; }

@keyframes amp-viewer-trace { 
  0% { stroke-dashoffset: 0; transform: rotate(0deg); }
  100% { stroke-dashoffset: 226px; transform: rotate(-360deg); }
