.amp-viewer-frame-loading, .amp-viewer-frame-custom-markup { position: absolute; width: 100%; height: 100%; top: 0px; left: 0px; }

.amp-viewer-frame-custom-markup { overflow: scroll; }

.amp-viewer-frame-loading { z-index: 2; background: rgb(255, 255, 255); opacity: 0; pointer-events: none; transition: opacity 0.25s ease-in 0s; }

.amp-viewer-frame-loading.amp-viewer-frame-loading-active { opacity: 1; }

.amp-viewer-frame-loading.amp-viewer-frame-loading-active + iframe { display: none; }

.amp-viewer-frame-loading .amp-viewer-loading-indicator { position: absolute; top: 50%; left: 50%; margin-left: -20px; margin-top: -20px; }

