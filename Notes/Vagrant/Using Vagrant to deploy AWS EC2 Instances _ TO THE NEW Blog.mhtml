@charset "utf-8";

fieldset { border: 1px solid rgba(0, 0, 0, 0.1); margin: 0px 0px 24px; padding: 11px 12px 0px; }

legend { white-space: normal; }

button, input, select, textarea { box-sizing: border-box; font-size: 100%; margin: 0px; max-width: 100%; vertical-align: baseline; }

button, input { line-height: normal; }

input, textarea { border-top: none; border-right: none; border-left: none; border-image: initial; border-bottom: 1px solid rgb(162, 167, 174); height: 48px; outline: none; width: 100%; resize: none; font-size: 15px; }

button, html input[type="button"], input[type="reset"], input[type="submit"] { -webkit-appearance: button; cursor: pointer; }

button[disabled], input[disabled] { cursor: default; }

input[type="checkbox"], input[type="radio"] { padding: 0px; }

input[type="search"] { -webkit-appearance: textfield; }

input[type="search"]::-webkit-search-decoration { -webkit-appearance: none; }

textarea { overflow: auto; vertical-align: top; }

#submit { background: rgb(217, 22, 126); border: 1px solid rgb(217, 22, 126); color: rgb(255, 255, 255); font-family: open_sansregular; font-size: 14px; height: 45px; line-height: 42px; padding: 0px 57px; width: auto; margin-top: 34px; transition: all 0.5s ease 0s; }

#submit:hover { background: rgb(180, 10, 101); color: rgb(255, 255, 255); border-color: rgb(180, 10, 101); }

.comments-area { max-width: 100%; }

.comments-title { text-transform: uppercase; text-align: center; font-size: 19px !important; margin: 0px !important; padding-bottom: 20px !important; }

.comment-reply-title { margin: 0px; text-transform: uppercase; text-align: left; color: rgb(51, 60, 78); font-weight: 700; letter-spacing: 2px; font-size: 12px !important; font-family: "Open Sans" !important; }

.commentlist { list-style: none; margin: 0px 0px 95px; padding: 0px; text-align: left; }

.commentlist section { text-align: left; font-size: 14px; color: rgb(51, 60, 78); }

.comment-author { font-size: 14px; line-height: 1.71429; }

.comment-author a { clear: left; margin-left: 5px; font-size: 10px; color: rgb(156, 155, 155); font-family: open_sansregular; }

.comment-author a::before { content: "-"; }

.commentlist .reply, .comment-metadata { font-size: 12px; line-height: 2; text-transform: uppercase; }

.logged-in-as { padding-bottom: 10px; }

ol.commentlist li.comment { position: relative; margin-bottom: -1px; }

.commentlist .reply { right: 1px; position: absolute; bottom: 1px; background: rgb(255, 255, 255); border-radius: 5px; color: rgb(217, 22, 126); font-size: 14px; border: 1px solid rgb(217, 22, 126); font-family: "Open Sans"; line-height: 22px; }

.commentlist .reply:hover, .commentlist .reply:focus { background: rgb(217, 22, 126); }

.commentlist .reply:hover a { color: rgb(255, 255, 255); }

.commentlist .reply span { display: none; }

.comment-author .fn { font-weight: 900; }

.comment-author a { color: rgb(156, 155, 155); }

.commentlist .trackback a, .commentlist .pingback a, .comment-metadata a { color: rgb(118, 118, 118); }

.comment-author a:hover, .commentlist .pingback a:hover, .commentlist .trackback a:hover, .comment-metadata a:hover { color: rgb(156, 155, 155); }

.commentlist article, .commentlist .pingback, .commentlist .trackback { padding-bottom: 45px; padding-top: 24px; position: relative; }

.commentlist > li:first-child > article, .commentlist > .pingback:first-child, .commentlist > .trackback:first-child { border-top: 0px; }

.comment-author { position: relative; }

.comment-author cite { display: inline-block; }

.comment-author .avatar, .comment-author img { border: 1px solid rgba(0, 0, 0, 0.1); height: 53px; padding: 0px; position: absolute; top: 0px; left: 0px; width: 53px; border-radius: 53px; overflow: hidden; }

.bypostauthor > article .fn::before { margin: 0px 2px 0px -2px; position: relative; top: -1px; }

.says { display: none; }

.comment-author, .comment-awaiting-moderation, .comment-content, .comment-metadata { padding-left: 70px; max-width: 518px; }

.comment-edit-link, .logged-in-as a { color: rgb(217, 22, 126); }

.comment-reply-link { text-transform: capitalize; color: rgb(217, 22, 126); padding: 5px 30px; display: block; }

.comment-reply-link:hover, .comment-reply-link:focus { color: rgb(255, 255, 255); text-decoration: none; }

.comment-content { padding: 0px 0px 0px 70px; margin: 10px 0px 0px; max-width: 518px; }

.comment-content ul, .comment-content ol { margin: 0px 0px 24px 22px; }

.comment-content li > ul, .comment-content li > ol { margin-bottom: 0px; }

.comment-content > :last-child { margin-bottom: 0px; }

.commentlist .children { list-style: none; margin-left: 15px; }

.comment-respond { margin-bottom: 0px; padding: 0px; }

.comment .comment-respond { margin-top: 24px; }

.comment-respond h3 { margin-top: 0px; margin-bottom: 24px; }

.comment-notes, .comment-awaiting-moderation, .logged-in-as, .no-comments, .form-allowed-tags, .form-allowed-tags code { color: rgb(118, 118, 118); }

.comment-notes, .comment-awaiting-moderation, .logged-in-as { font-size: 14px; line-height: 1.71429; }

.no-comments { font-size: 16px; font-weight: 900; line-height: 1.5; margin-top: 24px; text-transform: uppercase; }

.comment-form label { display: block; }

.comment-form input[type="text"], .comment-form input[type="email"], .comment-form input[type="url"] { width: 100%; }

