#tos__container #tos__message span { display: inline-block; width: calc(100% - 25px); vertical-align: top; margin-top: 3px; }

#tos__container #tos__message > p:last-of-type { margin-bottom: 0px; }

#tos__container #tos__message .checkbox-wrapper { max-width: 600px; }

#tos__container #accept_tos { background-color: buttonface; margin: 10px auto 0px; }
