.amp-viewer-target-open { overflow: hidden; }

.amp-viewer-target-open:not(.amp-viewer-exiting) > :not(.amp-viewer-wrapper) { display: none; }

.amp-viewer-wrapper { display: none; position: fixed; width: 100%; height: 100%; top: 0px; left: 0px; font-family: sans-serif; z-index: 10000; overflow: hidden; transform: translate3d(0px, 0px, 0px); transition: transform 400ms cubic-bezier(0.25, 0.21, 0, 1) 0s, opacity 400ms cubic-bezier(0.25, 0.21, 0, 1) 0s; }

