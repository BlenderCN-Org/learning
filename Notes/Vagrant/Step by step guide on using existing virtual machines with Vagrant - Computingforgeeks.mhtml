@charset "utf-8";

html, body { font-family: Arial, san-serif; font-weight: 700; height: 100%; margin: 0px; padding: 0px; width: 100%; }

img { border: none; }

#adContent { height: 100%; overflow: hidden; padding-top: 17px; }

.core { box-sizing: border-box; overflow: hidden; padding: 0px 4px 8px 0px; position: relative; }

.core a { color: inherit; display: block; width: 100%; height: 100%; }

.core-img img { height: 100%; margin: auto; display: block; }

.core-img { background: rgb(194, 194, 194); }

#adContent.loaded .core-img { background: transparent; }

.core-title a { display: block; text-decoration: none; }

.core-title a:hover { text-decoration: underline; }

.core-title .sponsored { font: 300 12px/1.2 Roboto; color: rgb(132, 132, 132); padding-bottom: 3px; }

.core-body { box-sizing: border-box; overflow: hidden; padding-top: 4px; line-height: 1.2; }

.core-ad-pill { white-space: nowrap; display: inline-block; padding-top: 3px; }

.core-ad-pill .ad-badge, .sponsoredByGoogle { background-color: rgb(179, 179, 179); color: rgb(255, 255, 255); font: 500 11px/13px Roboto; border-radius: 2px; padding: 2px; }

.core-ad-pill .url-or-advertiser { overflow: hidden; text-overflow: ellipsis; font: 300 11px/17px Roboto; color: rgb(179, 179, 179); display: block; vertical-align: text-bottom; text-decoration: none; width: auto; }

.core-ad-pill-overlay { background-color: rgb(255, 255, 255); font-size: 13px; top: 4px; left: 4px; padding-left: 3px; padding-right: 3px; border-radius: 2px; position: absolute; box-shadow: rgba(0, 0, 0, 0.5) 0px 1px 2px; }

.sponsoredByGoogle { display: inline; padding-right: 4px; padding-left: 4px; float: left; }

.sponsoredByGoogle.rtl { float: right; }

.radlink { border-bottom: 1px solid rgb(230, 230, 230); overflow: hidden; padding: 0px 0px 4px; position: relative; margin: 0px 0px 4px; text-overflow: ellipsis; }

.radlink a { color: inherit; text-decoration: none; }

.radlinkFrame { border: 1px solid rgb(230, 230, 230); position: absolute; padding: 4px; }

.radlinks { float: left; }

.radlinks.rtl { float: right; }

.radlink:last-child { border-bottom: none; padding-bottom: 0px; }
